                        </div>

                        <!-- Coupon Buttons -->
                            <% let couponStatus = appliedCoupon ? "Remove Coupon" : "Apply Coupon"; %>
                            <input type="text" placeholder="Coupon code" id="couponInput" <% if(appliedCoupon){ %> value="<%= appliedCoupon.couponCode %>" readonly <% } %> oninput="validateCoupon()" onchange="validateCoupon()">

                            <% if(!appliedCoupon){ %>
                                <button disabled class="text-muted applyCouponBtn" onclick="applyCoupon(this)"  id="applyCouponId"><%= couponStatus %></button>
