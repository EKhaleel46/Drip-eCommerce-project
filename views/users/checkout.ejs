                                <button class=" applyCouponBtn" onclick="removeCoupon(this)"  id="applyCouponId" ><%= couponStatus %></button>
                            <% } %>

                            <button disabled hidden class="text-muted applyCouponBtn" onclick="applyCoupon(this)"  id="applyCouponId1"><%= couponStatus %></button>
                            <button class=" applyCouponBtn" hidden onclick="removeCoupon(this)"  id="remove" >Remove Coupon</button>
