        async function removeAddress(addressId) {
            const result = await Swal.fire({
                title: "Are you sure?",
                text: "Remove this Address",
                icon: "warning",
                showCancelButton: true,
                confirmButtonColor: "#3085d6",
                cancelButtonColor: "#d33",
                confirmButtonText: "Delete"
            })
            if(result.isConfirmed){
                try{
                    const response = await axios.patch('/removeAddress',{addressId})

                    if(response.data.loginRequired){
                        window.location.href = '/login'
                    }else{
                        if(response.data.success){
                            $('#shippingArea').load('/checkout #shippingArea');
                        }
                    }

                }catch(error){
                    console.log(error.message);
                }
            }
        }

        async function placeOrder(){

            const paymentMethodOpt = document.querySelector('input[name="paymentMethod"]:checked')
            const addressSelection = document.querySelector('input[name="addressSelected"]:checked')
            let orderAmount = document.getElementById('grandTotalVal').textContent
            // if paypal
            const subTotal =  document.getElementById('subTotal')
            const discountPrice =  document.getElementById('DiscountPrice')

            console.log(' discssss ');
            console.log(subTotal.value);
            console.log(discountPrice.value);
            

            document.getElementById('errorPlacing').innerHTML = '';
            document.getElementById('addrssErrorPlacing').innerHTML = '';

            if(!paymentMethodOpt){
                document.getElementById('errorPlacing').innerHTML = 'Please select a Payment option';
                return false
            } else if(!addressSelection){
                document.getElementById('addrssErrorPlacing').innerHTML = "Please select an Address option"
                return false
            }

            const paymentMethod = paymentMethodOpt.value
            const selectedAddress = addressSelection.value

            if(paymentMethod == "Online Payment"){
                console.log('enter the function');
                try{
                    const response = await axios.post('/paypal',{
                        paymentMethod,
                        selectedAddress,
                        orderAmount,
                        discountPrice:discountPrice.value,
                        subTotal:subTotal.value,
                    });

                    if(response.data.loginRequired){
                        window.location.href = '/login'
                    }else{
                        const redirectUrl = response.data.redirectUrl;
                        window.location.href = redirectUrl;
                        console.log('after everything')
                    }

                }catch(error){
                    console.log(error.message);
                }

            } else if(paymentMethod == "Cash on Delivery"){

                if(orderAmount<100){
                    document.getElementById('errorPlacing').innerHTML = 'COD only available for orders above $100';
                } else {
                    const response = await axios.post('/COD',{paymentMethod,selectedAddress,orderAmount,discountPrice:discountPrice.value,subTotal:subTotal.value})
                    if(response.data.loginRequired){
                        window.location.href = '/login'
                    }else{
                        if(response.data.success){
                            window.location.href = '/successMessage'
                        }
                        console.log("order amount at place order");
                        console.log(orderAmount);
                    }
                }
            } else if(paymentMethod == "Wallet"){
                const response = await axios.post('/COD',{paymentMethod,selectedAddress,orderAmount,discountPrice:discountPrice.value,subTotal:subTotal.value})

                if(response.data.loginRequired){
                    window.location.href = '/login'
                }else{
                    if(response.data.success){
                        window.location.href = '/successMessage'
                    }
                }
            }

        }


