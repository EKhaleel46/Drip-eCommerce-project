        async function validateCoupon() {

            const couponCode = document.getElementById('couponInput').value.trim();
            console.log(couponCode);

            const couponErr = document.getElementById('couponCodeErrorMes');
            const applyBtn = document.getElementById('applyCouponId');

            // Reset error message
            couponErr.innerHTML = "";       

            if (couponCode.length === 0) {
                applyBtn.setAttribute('disabled', 'disabled');
                applyBtn.classList.add('text-muted');
                return;
            }

            const response = await axios.patch('/validateCoupon', { couponCode });

            if(response.data.loginRequired){
                window.location.href = '/login'
            }else{
                if(response.data.expired){
                    couponErr.innerHTML = "Coupon Expired";
                    couponErr.classList.remove('d-none');
                    couponErr.classList.add('text-danger');
                    applyBtn.setAttribute('disabled', 'disabled');
                    applyBtn.classList.add('text-muted');
                } else if(response.data.success) {
                    couponErr.classList.add('d-none');
                    applyBtn.removeAttribute('disabled');
                    applyBtn.classList.remove('text-muted');
                } else if(response.data.notFound){
                    couponErr.innerHTML = "Coupon code doesn't match";
                    couponErr.classList.remove('d-none');
                    couponErr.classList.add('text-danger')
                    applyBtn.setAttribute('disabled', 'disabled');
                    applyBtn.classList.add('text-muted');
                }
            }
        }


        async function applyCoupon(e){
            e.hidden=true
            const removeBtn = document.getElementById('remove')
            let grandTotalVal = document.getElementById('grandTotalVal').textContent
            const couponCode = document.getElementById('couponInput')
            removeBtn.hidden=false

            // coupon msg
            const couponSucc = document.getElementById('couponCodeSuccessMes');
            // Reset error message
            couponSucc.innerHTML = "";

            console.log(" tot ::: "+grandTotalVal);
            console.log(couponCode.value);

            const response = await axios.patch('/applyCoupon',{grandTotal:parseFloat(grandTotalVal),couponCode:couponCode.value})

            if(response.data.loginRequired){
                window.location.href = '/login'
            }else{
                if(response.data.success){

                    const Toast = Swal.mixin({
                        toast: true,
                        background : "#000000",
                        position: "bottom",
                        showConfirmButton: false,
                        width : "20vw",
                        timer: 4000,
                        timerProgressBar: true,
                    });

                    Toast.fire({
                        icon: "success",
                        title: "Coupon applied"
                    });

                    // assigning the discounted price to total variable
                    const grandTotal = document.getElementById('grandTotal')
                    grandTotal.innerHTML = "$" + response.data.grandTotaltot.toFixed(2)

                    let span = document.getElementById('discountPrice')
                    span.textContent =  "- $" + response.data.discountPrice.toFixed(2)

                    // assigning the discounted price to total hidden variable
                    let grandTotalVal1 = document.getElementById('grandTotalVal')
                    grandTotalVal1.textContent = response.data.grandTotaltot.toFixed(2)

                    let discountPricee = document.getElementById('DiscountPrice')
                    discountPricee.value = response.data.discountPrice
                    //
                    console.log(" after calculating :: "+grandTotalVal1);

                    // changing the attributes
                    const btn =  document.getElementById('applyCouponId')
                    couponCode.readOnly = true

                }
            }
            
        }

        async function removeCoupon(e){
            // const couponCode = document.getElementById('couponInput').value
            const applyBtn = document.getElementById('applyCouponId1')
            applyBtn.hidden=false
            const removeBtn = document.getElementById('remove')
            e.hidden = true

            const response = await axios.patch('/removeCoupon')

            if(response.data.loginRequired){
                window.location.href = '/login'
            }else{

                if(response.data.success){

                    const Toast = Swal.mixin({
                        toast: true,
                        background : "#000000",
                        position: "bottom",
                        showConfirmButton: false,
                        width : "20vw",
                        timer: 4000,
                        timerProgressBar: true,
                    });

                    Toast.fire({
                        icon: "success",
                        title: "Coupon removed"
                    });

                    // updating prices
                    const inputBox = document.getElementById('couponInput')
                    let span = document.getElementById('discountPrice')

                    inputBox.value = ""
                    inputBox.readOnly = false
                    span.textContent =  "- $0"

                    // assigning the deducted price to total
                    const grandTotal = document.getElementById('grandTotal')
                    const subtotal = document.getElementById('subTotal').value

                    grandTotal.innerHTML = "$" + subtotal

                    // assigning the deducted price to hidden total
                    let grandTotalVal1 = document.getElementById('grandTotalVal')
                    grandTotalVal1.textContent = subtotal

                    const applyCpnBtn = document.getElementById('applyCouponId')
                    applyBtn.setAttribute('disabled', 'disabled')
                    applyBtn.classList.add('text-muted')

                    
                }
            }
        }
        
