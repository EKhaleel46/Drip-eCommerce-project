                                    <div class="modal-content">
                                        <div class="modal-header">
                                            <h4 class="modal-title">Edit Profile</h4>
                                            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                                        </div>
                                        <div class="modal-body p-4">
                                                
                                                    <div class="mb-3">
                                                        <label for="name" class="form-label">Name :</label>
                                                        <input type="text" class="form-control" id="editNameId"
                                                            name="editName" value="<%= userData.name %>" >
                                                            <p id="editNameErr" class="text-danger "></p>
                                                    </div>
                                                    <div class="mb-3">
                                                        <label for="pincode" class="form-label">Phone :</label>
                                                        <input type="number" class="form-control" id="editPhoneId"
                                                            name="editPhone" value="<%= userData.phone %>">
                                                            <p id="editPhoneErr" class="text-danger "></p>
                                                    </div>
                                                    <p id="emptyError" class="text-danger"></p>
                                        </div>
                                        <div class="modal-footer">
                                            <button type="button" class="btn btn-secondary" id="close" data-bs-dismiss="modal">Close</button>
                                            <button type="submit" id="saveEditUser"  class="btn btn-primary" onclick="editUser(this)">Save changes</button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!-- Edit User Modal End -->
                            <!-- Single Tab Content End -->
                            <!-- Single Tab Content Start -->
                            <div class="tab-pane fade" id="orders" role="tabpanel">
                                <div class="myaccount-content">
                                    <% if(orders.length >= 1){ %>
                                    <div class="myaccount-table table-responsive text-center">
                                        <% orders.forEach((order)=>{ %>
                                            <div class="d-flex mb-4 bg-light border  justify-content-between p-3">
                                                <div class="d-block ps-3" style="max-width: 100px;">
                                                    <h5 class="fw-semibold "><%= order.products.length %> Items</h5>
                                                    <div>
                                                        <% order.products.forEach((prod)=>{ %>
                                                            <img src="/uploads/<%= prod.productId.images[0] %>" style="width:60px; border-radius: 3px;" alt="">
                                                        <% }) %>
                                                    </div>
                                                </div>
                                                <div >
                                                    <p><%= order.orderDate.toLocaleDateString() %></p>
                                                    <p>$<%= order.orderAmount.toFixed(2) %></p>
                                                </div>
                                                <div class="pe-2">
                                                    <a type="button" class="btn btn-dark" href="/orderDetails/<%= order._id %>">View</a>
                                                </div>                
                                            </div>
                                        <% }) %>
                                    </div>
                                    <% }else{ %>
                                        <div>
                                            <p class="text-center text-danger ">No Orders</p>
                                        </div>
                                    <% } %>
                                </div>
                            </div>
                            <!-- Single Tab Content End -->
                            <!-- Single Tab Content Start -->
