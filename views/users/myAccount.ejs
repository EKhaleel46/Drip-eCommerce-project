        async function removeAddress(addressId) {
            const result = await Swal.fire({
                title: "Are you sure?",
                text: "Remove this Address",
                icon: "warning",
                showCancelButton: true,
                confirmButtonColor: "#3085d6",
                cancelButtonColor: "#d33",
                confirmButtonText: "Delete"
            })

            if(result.isConfirmed){
                try{
                    const response = await axios.patch('/removeAddress',{addressId})
                    
                    if (response.data.loginRequired) {
                        window.location.href = '/login';
                    } else if (response.data.success) {
                        $('#reloadaddress').load('/myAccount #reloadaddress');
                    }
                }catch(error){
                    console.log(error.message);
                }
            }
            
        }

        async function addMoney(){
            const amount = document.getElementById('addAmountId').value

            document.getElementById('addAmountErr').innerHTML = ""

            if(amount === ''){
                document.getElementById('addAmountErr').innerHTML = "Enter amount"
                return false
            }

            document.getElementById('addAmountErr').innerHTML = ""

            const response = await axios.patch('/addAmount',{amount})
            
            if(response.data.loginRequired){
                window.location.href = '/login'
            }else{

                if(response.data.success){
                    const Toast = Swal.mixin({
                        toast: true,
                        background : "#000000",
