    <script>

        function checkStock(e,stock){

            const inp = e.querySelector('input')

            if(stock<Number(inp.value)){
                inp.value=stock;

                const Toast = Swal.mixin({

                    toast: true,
                    position: "bottom",
                    showConfirmButton: false,
                    timer: 1600,
                    timerProgressBar: true,

                    didOpen: (toast) => {
                        toast.onmouseenter = Swal.stopTimer;
                        toast.onmouseleave = Swal.resumeTimer;
                    }
                });

                Toast.fire({

                icon: "warning",
                title: "Reached stock limit"

                });
            }

            if(Number(inp.value)<=0) inp.value=1
        }

        async function addToCart(productId){
            try{
                const quantity = document.getElementById('quantity').value

                const response = await axios.post('/addToCart',{productId,quantity})
                
                if(response.data.loginRequired){
                    window.location.href = '/login'
                } else {

                    if(response.data.exist){
                    
                        const Toast = Swal.mixin({
                            toast: true,
                            position: "top-end",
                            showConfirmButton: false,
                            timer: 1600,
                            timerProgressBar: true,

                        didOpen: (toast) => {

                            toast.onmouseenter = Swal.stopTimer;
                            toast.onmouseleave = Swal.resumeTimer;
                        }

                        });

                        Toast.fire({

                            icon: "warning",
                            title: "Already Added"

                        });

                    }else if(response.data.success){
                        
                        const Toast = Swal.mixin({

                            toast: true,
                            position: "top-end",
                            showConfirmButton: false,
                            timer: 1600,
                            timerProgressBar: true,

                            didOpen: (toast) => {
                                toast.onmouseenter = Swal.stopTimer;
                                toast.onmouseleave = Swal.resumeTimer;
                            }
                        });

                        Toast.fire({

                            icon: "success",
                            title: "Item added to cart"
                            
                        });

                        $('#shoppingCartid').load(`/productDetail/${productId} #shoppingCartid`)

                    }
                }

            }catch(error){
                console.log(error.message);
            }
        }

        async function addToWishlist(productId){
            try{
                const response = await axios.patch('/addToWishlist',{productId})

                if(response.data.loginRequired){
                    window.location.href = '/login'
                } else{
                    if(response.data.success){
                        const Toast = Swal.mixin({

                            toast: true,
                            position: "top-end",
                            showConfirmButton: false,
                            timer: 2000,
                            timerProgressBar: true,
                        });

                            Toast.fire({

                            icon: "success",
                            title: "Added to Wishlist"

                            });

                            $('#shoppingCartid').load(`/productDetail/${productId} #shoppingCartid`)

                    } else if(response.data.exist){
                        const Toast = Swal.mixin({
                                toast: true,
                                position: "top-end",
                                showConfirmButton: false,
                                timer: 1600,
                                timerProgressBar: true
                            });

                            Toast.fire({

                                icon: "warning",
                                title: "Already Added"

                            });
                    }
                }
            }catch(error){
                console.log(error.message)
            }

        }
    </script>
